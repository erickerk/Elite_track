{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VERCEL_TOOLBAR_ENABLED": "false",
    "VERCEL_ANALYTICS_ID": ""
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-vercel-skip-toolbar",
          "value": "1"
        }
      ]
    }
  ]
}
